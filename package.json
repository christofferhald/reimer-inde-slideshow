{
  "name": "reimer-inde-slideshow",
  "version": "1.0.2",
  "description": "Slideshow for Reimer-Inde website. Requires Advanced Custom Fields Pro.",
  "main": "./srcx/js/index.js",
  "scripts": {
	"switch": "npm install && npm rebuild node-sass && npm audit fix",
    "watch": "gulp watch && webpack --watch --config webpack.dev.js",
    "w": "npm run watch",
    "build": "webpack --config webpack.prod.js && gulp build",
    "buildnopurge": "webpack --config webpack.prod.js && gulp buildnopurge",
    "b": "npm run build",
    "updates": "ncu",
    "updates-major": "ncu -u --target latest && npm install",
    "updates-minor": "ncu -u --target minor && npm install",
    "updates-patch": "ncu -u --target patch && npm install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Christoffer Helgelin Hald",
  "license": "ISC",
  "dependencies": {
    "swiper": "^8.2.6"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "browser-sync": "^2.27.10",
    "browser-sync-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.12",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-inject-string": "^1.1.2",
    "gulp-minify": "^3.1.0",
    "gulp-postcss": "^9.0.1",
    "gulp-purgecss": "^4.1.3",
    "gulp-sass": "^5.1.0",
    "mini-css-extract-plugin": "^2.6.1",
    "node-sass": "^7.0.1",
    "postcss-loader": "^6.2.1",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  }
}
